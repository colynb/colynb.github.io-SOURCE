<!DOCTYPE html>
<!--[if IEMobile 7 ]><html class="no-js iem7"><![endif]-->
<!--[if lt IE 9]><html class="no-js lte-ie8"><![endif]-->
<!--[if (gt IE 8)|(gt IEMobile 7)|!(IEMobile)|!(IE)]><!--><html class="no-js" lang="en"><!--<![endif]-->
<head>
  <!-- Standard Meta -->
  <meta charset="utf-8" />

  <!-- Site Properities -->
  <title><%= @getPreparedTitle() %></title>
  <meta name="description" content="<%= @getPreparedDescription() %>" />
  <meta name="keywords" content="<%= @getPreparedKeywords() %>" />

  <!-- DocPad Meta -->
  <%- @getBlock('meta').toHTML() %>

  <!-- DocPad Styles + Our Own -->
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css">
  <%- @getBlock('styles').add(@site.styles).toHTML() %>


</head>

<body>

  <!--[if lt IE 7]>
        <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
    <![endif]-->

  <header role="banner" id="sidebar">
    <!-- Logo -->
<aside id="logo" class="clearfix">
  <div class="clearfix">
    <a href="/">Colyn Brown</a>
  </div>
</aside>

<ul id="menu">

  <li class="title">
    <h1 id="title"><a href="/">Colyn Brown</a></h1>
  </li>


  <li class="subtitle">
    <h2 id="subtitle">Front-end Development and Consulting Blog.</h2>
  </li>

  <% for post in @getFilesAtPath("pages").toJSON(): %>
    <li class="link"><a href="<%= post.url %>"><%= post.title %></a></li>
  <% end %>

  <li class="link"><a href="https://twitter.com/colynb"><img src="/img/twitter-small.png" style="vertical-align: middle;">&nbsp;@colynb</a></li>


</ul>


<!-- Octopress Love -->
<aside>
    <span class="unicode_square">
      <span class="unicode_circle">
        &nbsp;
      </span>
    </span>
    <span class="docpad"><a href="http://docpad.org//">Powered by Docpad</a></span>
    <p><small>&copy; 2013 Colyn Brown</small></p>
</aside>

  </header>
  <section id="main">
      
  
  
    <article class="post">


<%- @content %>
  


    </article>
  

    <nav role="navigation" id="pagination">

</nav>
  </section>

<%- @getBlock("scripts").toHTML() %>
<script type="text/javascript">
    /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
    var disqus_shortname = 'colynb-com'; // required: replace example with your forum shortname

    /* * * DON'T EDIT BELOW THIS LINE * * */
    (function () {
        var s = document.createElement('script'); s.async = true;
        s.type = 'text/javascript';
        s.src = '//' + disqus_shortname + '.disqus.com/count.js';
        (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
    }());
    </script>
    
</body>
</html>
